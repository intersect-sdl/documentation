licor:LI8250_01 a sosa:Platform, ssn:System ;
    rdfs:label "LI-8250-01 Extension Manifold" ;
    dct:description "Extension Manifold accessory for LI-8250 Multiplexer system, responsible for distributing sample flow and controlling pump operations." ;
    sosa:hosts licor:PumpCurrentSensor,
               licor:VoltageInputSensor,
               licor:TemperatureManifoldSensor,
               licor:SolenoidCurrentSensor ;
    dct:identifier "SN-8250-01-EXM-001" ;
    licor:firmware "v1.4.3" ;
    licor:port "2" ;
    licor:tubeLength "120"^^xsd:decimal ;  # cm
    licor:volume "450"^^xsd:decimal .      # cm^3

licor:PumpCurrentSensor a sosa:Sensor ;
    rdfs:label "Pump Current Sensor" ;
    sosa:observes licor:PumpCurrent .

licor:VoltageInputSensor a sosa:Sensor ;
    rdfs:label "Input Voltage Sensor" ;
    sosa:observes licor:VoltageIn .

licor:TemperatureManifoldSensor a sosa:Sensor ;
    rdfs:label "Manifold Temperature Sensor" ;
    sosa:observes licor:TemperatureManifold .

licor:SolenoidCurrentSensor a sosa:Sensor ;
    rdfs:label "Solenoid Current Sensor" ;
    sosa:observes licor:SolenoidCurrent .

### OBSERVABLE PROPERTIES

licor:TemperatureCase a sosa:ObservableProperty ;
    rdfs:label "Internal Temperature (T_CASE)" ;
    dct:description "Internal temperature of the multiplexer" ;
    qudt:unit unit:DEG_C .

licor:PressureAtmospheric a sosa:ObservableProperty ;
    rdfs:label "Atmospheric Pressure (PA)" ;
    dct:description "" ;
    qudt:unit unit:KiloPA .

licor:PressureTemperature a sosa:ObservableProperty ;
    rdfs:label "Pressure Sensor Temperature (T_PA)" ;
    dct:description "Atmospheric pressure sensor temperature" ;
    qudt:unit unit:DEG_C .

licor:PumpPositivePressure a sosa:ObservableProperty ;
    rdfs:label "Positive Pump Pressure" ;
    dct:description "Pressure sensor on the positive pressure side of the sampling pump" ;
    qudt:unit unit:KiloPA .

licor:PumpNegativePressure a sosa:ObservableProperty ;
    rdfs:label "Negative Pump Pressure" ;
    dct:description "Pressure sensor on the negative pressure side of the sampling pump" ;
    qudt:unit unit:KiloPA .

licor:FlowPressure a sosa:ObservableProperty ;
    rdfs:label "Flow Pressure" ;
    dct:description "Pressure measurement used to determine flow rate" ;
    qudt:unit unit:KiloPA .

licor:SubsampleFlowPressure a sosa:ObservableProperty ;
    rdfs:label "Subsample Flow Pressure" ;
    dct:description "Pressure measurement used to determine subsample flow rate" ;
    qudt:unit unit:KiloPA .

licor:Latitude a sosa:ObservableProperty ;
    rdfs:label "Latitude" ;
    dct:description "Latitude reported by GPS" ;
    qudt:unit unit:DEG .

licor:Longitude a sosa:ObservableProperty ;
    rdfs:label "Longitude" ;
    dct:description "Longitude reported by GPS" ;
    qudt:unit unit:DEG .

licor:Altitude a sosa:ObservableProperty ;
    rdfs:label "Altitude" ;
    dct:description "Altitude reported by GPS" ;
    qudt:unit unit:M .

licor:Satellites a sosa:ObservableProperty ;
    rdfs:label "Number of Satellites" ;
    dct:description "Number of satellites" ;
    qudt:unit unit:UNITLESS .

licor:Flow a sosa:ObservableProperty ;
    rdfs:label "Flow Rate" ;
    dct:description "Flow rate through the LI-8250 Multiplexer to the active port." ;
    qudt:unit unit:L-PER-MIN .

licor:PumpSetpoint a sosa:ObservableProperty ;
    dct:description "Drive setting for the sampling pump" ;
    rdfs:label "Pump Setpoint" .

licor:PumpCurrent a sosa:ObservableProperty ;
    rdfs:label "Pump Current" ;
    csvw:columnName "PUMP_CURRENT" ;
    qudt:unit unit:AMP ;
    rdfs:comment "Current draw by the sampling pump." .

licor:SubsamplePressure a sosa:ObservableProperty ;
    rdfs:label "Subsample Pressure" ;
    dct:description "Ambient to subsample loop pressure differential" ;
    qudt:unit unit:KiloPA .

licor:SubsampleFlow a sosa:ObservableProperty ;
    rdfs:label "Subsample Flow Rate" ;
    dct:description "Flow through the subsample loop" ;
    qudt:unit unit:L-PER-MIN .

licor:VSOFlowSetpoint a sosa:ObservableProperty ;
    dct:description "Setting for the the inlet side of the subsample loop" ;
    rdfs:label "VSO Flow Setpoint" .

licor:VSOPressureSetpoint a sosa:ObservableProperty ;
    dct:description "Setting for the outlet side of the subsample loop" ;
    rdfs:label "VSO Pressure Setpoint" .

licor:PortCurrent a sosa:ObservableProperty ;
    rdfs:label "Port Current" ;
    dct:description "Current draw through the port number listed" ;
    qudt:unit unit:AMP .

licor:SolenoidCurrent a sosa:ObservableProperty ;
    rdfs:label "Solenoid Current" ;
    dct:description "Current supplied to valves and VSOs" ;
    qudt:unit unit:AMP .

licor:Current12VDC a sosa:ObservableProperty ;
    rdfs:label "12 VDC Current" ;
    dct:description "Current draw on the 12 VDC power output supply" ;
    qudt:unit unit:AMP .

licor:VoltageIn a sosa:ObservableProperty ;
    rdfs:label "Voltage In (VIN)" ;
