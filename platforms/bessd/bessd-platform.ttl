licor:NormalTemperatureCondition a ssn-system:Condition , schema:PropertyValue ;
  rdfs:comment "A temperature range of -40 to 80 Celsius."@en ;
  schema:minValue -20.0 ;
  schema:maxValue 45.0 ;
  schema:unitCode unit:DEG_C .

licor:NormalHumidityCondition a ssn-system:Condition , schema:PropertyValue ;
  rdfs:comment "A relative humidity range of 0 to 95, non-condensing %."@en ;
  schema:minValue 0.0 ;
  schema:maxValue 95.0 ;
  schema:unitCode unit:PERCENT .

licor:LI-8250-OperatingRange a ssn-system:OperatingRange ;
  rdfs:comment "The conditions in which the LI-8250 Multiplexer is expected to operate."@en ;
  ssn-system:inCondition licor:NormalTemperatureCondition , licor:NormalHumidityCondition .

licor:LI-7810 a ssn:System ;
  rdfs:label "LI-7810 CH4, H2O, CO2 gas analyzer"@en .

licor:LI-7820 a ssn:System ;
  rdfs:label "LI-7820 N2O/H2O gas analyzer"@en .

licor:LI-8250-01-OperatingRange a ssn-system:OperatingRange ;
  rdfs:comment "The conditions in which the LI-8250-01 Extension Manifold is expected to operate."@en ;
  ssn-system:inCondition licor:NormalTemperatureCondition , licor:NormalHumidityCondition .


