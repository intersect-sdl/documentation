@prefix acl: <http://example.org/acl/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

acl: a ssn:Platform ;
  dct:title "ACL Platform 01" ;
  dct:description "First deployment of the Autonomous Chemistry Laboratory (ACL) at ORNL." .

# Instrument definitions (shared components)
acl:ChemspeedSwingXL a ssn:System ;
  rdfs:label "ChemspeedSwingXL" ;
  dct:description "Programmable synthesis platform with solid and liquid dispensing." .

acl:BallMill a ssn:System ;
  rdfs:label "Ball Mill" ;
  dct:description "Automated ball mill for precursor homogenization and mechanical activation." .

acl:Furnace a ssn:System ;
  rdfs:label "Furnace" ;
  dct:description "Programmable furnace for high-temperature annealing of synthesized materials." .

acl:UHPLC_CMS a ssn:System ;
  rdfs:label "UHPLC_CMS" ;
  dct:description "Advion Avant UHPLC with UV/Vis, RI detectors, and Compact Mass Spectrometer." .

acl:GC_HidenMS a ssn:System ;
  rdfs:label "GC_HidenMS" ;
  dct:description "Agilent 8990 GC with TCDs and optional Hiden QGA Mass Spectrometer." .

acl:XRD_Aeris a ssn:System ;
  rdfs:label "XRD_Aeris" ;
  dct:description "Malvern-Panalytical Aeris benchtop XRD for phase identification." .

acl:ElectrochemStation a ssn:System ;
  rdfs:label "ElectrochemStation" ;
  dct:description "Custom electrocatalytic testing station with Biologic SP-200." .

acl:TGA_DSC_MS a ssn:System ;
  rdfs:label "TGA_DSC_MS" ;
  dct:description "TA Instruments SDT-650 with mass spectrometer and modulated TGA/DSC." .

acl:LiqSepPlatform a ssn:System ;
  rdfs:label "LiqSepPlatform" ;
  dct:description "Liquid-liquid separation robotic platform with automated analysis." .

# Procedures and Observable Properties
acl:MillSampleProcedure a sosa:Procedure ;
  rdfs:label "Mill Sample" ;
  dct:description "Procedure to homogenize and activate precursor mixture using the ball mill." ;
  sosa:usedProcedure acl:BallMill ;
  sosa:hasInput acl:MillSpeed, acl:MillDuration .

acl:HeatSampleProcedure a sosa:Procedure ;
  rdfs:label "Heat Sample" ;
  dct:description "Procedure for annealing samples using a furnace." ;
  sosa:usedProcedure acl:Furnace ;
  sosa:hasInput acl:SetTemperature, acl:SoakTime, acl:Atmosphere .

acl:AnalyzeUHPLC a sosa:Procedure ;
  rdfs:label "UHPLC Analysis" ;
  dct:description "Procedure to analyze sample using UHPLC with CMS." ;
  sosa:usedProcedure acl:UHPLC_CMS ;
  sosa:hasInput acl:InjectionVolume, acl:ColumnTemperature .

acl:AnalyzeGC a sosa:Procedure ;
  rdfs:label "GC Analysis" ;
  dct:description "Procedure to analyze sample using Gas Chromatograph." ;
  sosa:usedProcedure acl:GC_HidenMS ;
  sosa:hasInput acl:CarrierGas, acl:OvenRamp .

acl:AnalyzeXRD a sosa:Procedure ;
  rdfs:label "XRD Analysis" ;
  dct:description "Procedure for powder X-ray diffraction measurement." ;
  sosa:usedProcedure acl:XRD_Aeris ;
  sosa:hasInput acl:ScanRange, acl:StepSize .

acl:RunElectrochem a sosa:Procedure ;
  rdfs:label "Electrochemical Test" ;
  dct:description "Procedure for conducting electrochemical measurements." ;
  sosa:usedProcedure acl:ElectrochemStation ;
  sosa:hasInput acl:PotentialWindow, acl:ScanRate .

acl:RunTGA a sosa:Procedure ;
  rdfs:label "TGA Analysis" ;
  dct:description "Procedure to analyze sample using thermal gravimetric analysis." ;
  sosa:usedProcedure acl:TGA_DSC_MS ;
  sosa:hasInput acl:HeatingRate, acl:MaxTemperature .

acl:RunLiqSep a sosa:Procedure ;
  rdfs:label "Liquid-Liquid Separation" ;
  dct:description "Procedure for automated biphasic extraction." ;
  sosa:usedProcedure acl:LiqSepPlatform ;
  sosa:hasInput acl:OrganicSolvent, acl:MixingSpeed .

acl:MillSpeed a sosa:ObservableProperty ;
  rdfs:label "Milling Speed" ;
  dct:description "Speed setting for the milling process in RPM." .

acl:MillDuration a sosa:ObservableProperty ;
  rdfs:label "Milling Duration" ;
  dct:description "Duration of the milling operation in minutes." .

acl:SetTemperature a sosa:ObservableProperty ;
  rdfs:label "Set Temperature" ;
  dct:description "Target temperature for annealing in degrees Celsius." .

acl:SoakTime a sosa:ObservableProperty ;
  rdfs:label "Soak Time" ;
  dct:description "Duration to hold the sample at the target temperature." .

acl:Atmosphere a sosa:ObservableProperty ;
  rdfs:label "Atmosphere" ;
  dct:description "Type of gas environment used during heating (e.g. air, Ar, N2, H2)." .

acl:InjectionVolume a sosa:ObservableProperty ;
  rdfs:label "Injection Volume" ;
  dct:description "Volume of sample injected into the UHPLC column (µL)." .

acl:ColumnTemperature a sosa:ObservableProperty ;
  rdfs:label "Column Temperature" ;
  dct:description "Temperature of the column during separation (°C)." .

acl:ScanRange a sosa:ObservableProperty ;
  rdfs:label "Scan Range" ;
  dct:description "Angular range of XRD scan in degrees (2θ)." .

acl:StepSize a sosa:ObservableProperty ;
  rdfs:label "Step Size" ;
  dct:description "Angular increment per step in the scan." .

acl:PotentialWindow a sosa:ObservableProperty ;
  rdfs:label "Potential Window" ;
  dct:description "Voltage range for electrochemical testing (e.g. -1 to +1 V)." .

acl:ScanRate a sosa:ObservableProperty ;
  rdfs:label "Scan Rate" ;
  dct:description "Rate of voltage scan during cyclic voltammetry (mV/s)." .

acl:CarrierGas a sosa:ObservableProperty ;
  rdfs:label "Carrier Gas" ;
  dct:description "Type of carrier gas used in the GC (e.g. He, Ar)." .

acl:OvenRamp a sosa:ObservableProperty ;
  rdfs:label "Oven Ramp" ;
  dct:description "Temperature ramp program for the GC oven." .

acl:HeatingRate a sosa:ObservableProperty ;
  rdfs:label "Heating Rate" ;
  dct:description "Rate at which the temperature increases (°C/min)." .

acl:MaxTemperature a sosa:ObservableProperty ;
  rdfs:label "Max Temperature" ;
  dct:description "Final temperature reached in the TGA program (°C)." .

acl:OrganicSolvent a sosa:ObservableProperty ;
  rdfs:label "Organic Solvent" ;
  dct:description "Type of organic solvent used in the biphasic system." .

acl:MixingSpeed a sosa:ObservableProperty ;
  rdfs:label "Mixing Speed" ;
  dct:description "Speed of mixing in revolutions per minute (RPM)." .

# System deployment to specific platforms with metadata
acl:Deploy01 a ssn:Deployment ;
  ssn:deployedSystem acl:ChemspeedSwingXL ;
  ssn:deployedOnPlatform acl: ;
  dct:identifier "CSXL-001" ;
  dct:description "Chemspeed" ;
  acl:firmwareVersion "v3.1.2" .

acl:Deploy02 a ssn:Deployment ;
  ssn:deployedSystem acl:UHPLC_CMS ;
  ssn:deployedOnPlatform acl: ;
  dct:identifier "UHPLC-045" ;
  acl:firmwareVersion "v1.9.0" .

acl:Deploy03 a ssn:Deployment ;
  ssn:deployedSystem acl:LiqSepPlatform ;
  ssn:deployedOnPlatform acl: ;
  dct:identifier "LLS-003" ;
  acl:firmwareVersion "v2.3-beta" .

acl:Deploy04 a ssn:Deployment ;
  ssn:deployedSystem acl:BallMill ;
  ssn:deployedOnPlatform acl:Deploy01 ;
  dct:identifier "BM-001" ;
  dct:description "Ball Mill integrated with Chemspeed SwingXL." ;
  acl:firmwareVersion "v1.2.5" .

# Declare additional data properties
acl:firmwareVersion a rdf:Property ;
  rdfs:label "firmware version" ;
  rdfs:domain ssn:Deployment ;
  rdfs:range xsd:string .

# Logical grouping of subsystems per platform
acl: ssn:hasSubSystem acl:ChemspeedSwingXL , acl:UHPLC_CMS , acl:GC_HidenMS , acl:XRD_Aeris , acl:ElectrochemStation , acl:TGA_DSC_MS .

# Sample Observation and Result
acl:Sample123 a sosa:FeatureOfInterest ;
  rdfs:label "Synthesized Solid Sample" ;
  dct:description "Ceramic powder sample synthesized via milling and furnace annealing." .

acl:XRDObservation001 a sosa:Observation ;
  sosa:madeBySensor acl:XRD_Aeris ;
  sosa:observedProperty acl:PhaseComposition ;
  sosa:hasFeatureOfInterest acl:Sample123 ;
  sosa:resultTime "2025-07-01T10:30:00Z"^^xsd:dateTime ;
  sosa:hasResult [
    rdf:value "Orthorhombic perovskite" ;
    dct:description "Phase identified from matched diffraction peaks."
  ] .

acl:PhaseComposition a sosa:ObservableProperty ;
  rdfs:label "Phase Composition" ;
  dct:description "Crystalline structure identified via XRD." .

acl:TGAObservation001 a sosa:Observation ;
  sosa:madeBySensor acl:TGA_DSC_MS ;
  sosa:observedProperty acl:MassLoss ;
  sosa:hasFeatureOfInterest acl:Sample123 ;
  sosa:resultTime "2025-07-01T11:00:00Z"^^xsd:dateTime ;
  sosa:hasResult [
    rdf:value "3.4"^^xsd:decimal ;
    dct:description "Percentage of mass lost up to 800°C under N2 atmosphere."
  ] .

acl:MassLoss a sosa:ObservableProperty ;
  rdfs:label "Mass Loss" ;
  dct:description "Total mass lost during heating." .

# Link observation to activity
acl:ActivitySynth001 a prov:Activity ;
  prov:used acl:MillSampleProcedure, acl:HeatSampleProcedure ;
  prov:generated acl:Sample123 ;
  prov:wasAssociatedWith acl:ChemspeedSwingXL ;
  prov:startedAtTime "2025-07-01T08:00:00Z"^^xsd:dateTime ;
  prov:endedAtTime "2025-07-01T10:00:00Z"^^xsd:dateTime .